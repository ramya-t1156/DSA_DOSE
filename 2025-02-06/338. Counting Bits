class Solution {
    public int countB(int n){
        int count = 0;
        while(n!=0){
            n &= (n-1);
            count++;
        }
        return count;
    }
    public int[] countBits(int n) {
        int[] countBits = new int[n+1];
        for(int i = 0;i<=n;i++){
            countBits[i] = countB(i);
        }
        return countBits;
    }
}
